{
  "name": "graph-of-thought",
  "version": "1.0.1",
  "description": "Advanced Graph-of-Thought implementation with BMSSP algorithm for vectorless document indexing and retrieval. Works standalone without API keys or external services. Includes backward-compatible Tree-of-Thought functionality.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.mjs",
      "require": "./dist/providers/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/providers/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/providers/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rag",
    "retrieval-augmented-generation",
    "llm",
    "ai",
    "tree-search",
    "document-retrieval",
    "vectorless",
    "reasoning",
    "tree-of-thought",
    "tot",
    "pageindex",
    "hierarchical-retrieval",
    "document-indexing",
    "text-search",
    "open-source",
    "standalone",
    "no-api-key"
  ],
  "author": "Gianne Bacay",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gpbacay/graph-of-thought.git"
  },
  "bugs": {
    "url": "https://github.com/gpbacay/graph-of-thought/issues"
  },
  "homepage": "https://github.com/gpbacay/graph-of-thought#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@google/generative-ai": "^0.24.1",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "openai": "^6.17.0",
    "tsup": "^8.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.2",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "@cyber2024/pdf-parse-fixed": "^1.1.1"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.20.0",
    "@google/generative-ai": ">=0.2.0",
    "openai": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@google/generative-ai": {
      "optional": true
    }
  }
}
